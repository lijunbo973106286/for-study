<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woniuxy.user.dao.UserDao">
    <insert id="register">
        insert into scfp_user values (null ,#{uname},#{password},#{name},#{identity},#{phonenum},#{email},#{enterprise_id},#{status},#{fid})
    </insert>
    <insert id="registertwo">
        insert into scfp_enterprise values (null,#{name},#{pay_pass},#{credit_code},#{legal_person},#{id_card},#{address},#{account_id},#{role},#{money},#{surplus},#{status})
    </insert>
    <insert id="registerthree">
        insert into scfp_enterprise_account values (null,#{bank},#{name},#{num},#{type},#{address},#{file_id},#{status},#{enterprisid})
    </insert>
</mapper>